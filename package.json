{
  "name": "norwegian-validation",
  "version": "1.0.0",
  "description": "Validation according to Norwegian rules",
  "main": "lib/index.js",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "build": "babel ./src --out-dir ./lib --source-maps --presets es2015,stage-2",
    "coverage": "tap --cov --coverage-report=lcov test/*.js",
    "prepublish": "babel ./src --out-dir ./lib --source-maps --presets es2015,stage-2 && browserify ./lib/index.js -o ./build/norwegian-validation.js --standalone NorwegianValidation && uglifyjs ./build/norwegian-validation.js --compress --mangle --output ./build/norwegian-validation.min.js --source-map ./build/norwegian-validation.min.js.map"
  },

  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "browserify": "^13.0.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.7.0",
    "tap": "^5.8.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "uglify-js": "^2.6.4"
  },
  "author": "JÃ¸rgen Landsnes",
  "license": "ISC"
}
